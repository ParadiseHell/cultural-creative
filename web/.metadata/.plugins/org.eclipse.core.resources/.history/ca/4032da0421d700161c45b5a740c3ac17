$(document).ready(function() {
	$('#culture-tab a').click(function (e) {
	  e.preventDefault()
	  $(this).tab('show')
	});
});

$('#btn-watch').on('click', function () {
	$(this).button('toggle');
    $("#").css("display");
});

$('#btn-modify').on('click', function () {
	$(this).button('toggle');
    $(this).button('complete');
});
